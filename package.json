{
  "name": "@gesslar/uglier",
  "version": "0.0.7",
  "description": "Composable ESLint flat config blocks for stylistic, JSDoc, and environment presets.",
  "type": "module",
  "main": "src/uglier.js",
  "exports": "./src/uglier.js",
  "bin": {
    "uglier": "./bin/install.js"
  },
  "files": [
    "src",
    "bin"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "style",
    "jsdoc",
    "globals",
    "404",
    "file",
    "not",
    "found",
    "cat.gif"
  ],
  "scripts": {
    "submit": "npm login && npm publish --access public",
    "update": "npx npm-check-updates -u && npm install",
    "pr": "gt submit --publish --restack --ai"
  },
  "license": "Unlicense",
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "@gesslar/colours": "^0.0.1",
    "@gesslar/toolkit": "^1.10.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "globals": "^16.5.0"
  },
  "devDependencies": {
    "@gesslar/uglier": "^0.0.6"
  }
}
