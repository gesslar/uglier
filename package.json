{
  "name": "@gesslar/uglier",
  "description": "Composable ESLint flat config blocks for stylistic, JSDoc, and environment presets.",
  "author": {
    "name": "gesslar",
    "email": "bmw@gesslar.dev",
    "url": "https://gesslar.dev"
  },
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gesslar/uglier.git"
  },
  "type": "module",
  "main": "src/uglier.js",
  "exports": "./src/uglier.js",
  "bin": {
    "uglier": "./bin/install.js"
  },
  "packageManager": "pnpm@10.26.2",
  "files": [
    "src",
    "bin",
    "UNLICENSE.txt"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "style",
    "jsdoc",
    "globals",
    "404",
    "file",
    "not",
    "found",
    "cat.gif"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "update": "pnpm up -L",
    "submit": "pnpm publish --access public --//registry.npmjs.org/:_authToken=\"${NPM_ACCESS_TOKEN}\"",
    "pr": "gt submit -p --ai",
    "patch": "pnpm version patch",
    "minor": "pnpm version minor",
    "major": "pnpm version major"
  },
  "license": "Unlicense",
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@gesslar/colours": "^0.0.1",
    "@gesslar/toolkit": "^3.4.0",
    "@skarab/detect-package-manager": "^1.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "globals": "^16.5.0"
  },
  "devDependencies": {
    "@gesslar/uglier": "^0.0.8",
    "eslint": "^9.39.2"
  }
}
